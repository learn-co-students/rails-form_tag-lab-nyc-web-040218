<%= form_tag students_path do %>
  <label>Student Form: Student First Name:</label><br>
  <%= text_field_tag :'students[first_name]' %><br>
 
  <label>Student Form: Student Last Name:</label><br>
  <%= text_field_tag :'students[last_name]' %><br>
 
  <%= submit_tag "Students Form Submit" %>
<% end %>

